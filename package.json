{
  "name": "v2ex",
  "version": "1.0.0",
  "private": true,
  "description": "another v2ex client",
  "templateInfo": {
    "name": "vuex",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "taro build --type weapp --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "start-devtools": "vue-devtools",
    "devweapp": "run-p start-devtools dev:weapp"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.11.1",
    "@tarojs/components": "3.0.7",
    "@tarojs/runtime": "3.0.7",
    "@tarojs/taro": "3.0.7",
    "tailwindcss": "^1.6.2",
    "taro-ui-vue": "^1.0.0-alpha.4",
    "timeago.js": "^4.0.2",
    "vue": "^3.0.0",
    "vue-class-component": "^7.2.5",
    "vue-template-compiler": "^2.5.0",
    "vuex": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@tarojs/mini-runner": "3.0.7",
    "@tarojs/webpack-runner": "3.0.7",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "@vue/devtools": "^5.3.3",
    "babel-preset-taro": "3.0.7",
    "eslint": "^7.6.0",
    "eslint-config-taro": "3.0.7",
    "eslint-plugin-vue": "^6.x",
    "npm-run-all": "^4.1.5",
    "stylelint": "15.10.1",
    "typescript": "^3.7.0",
    "vue-loader": "^15.9.2"
  }
}
